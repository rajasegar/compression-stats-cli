{
  "name": "compression-stats-cli",
  "version": "1.2.1",
  "description": "A CLI tool to print gzip and brotli compression statistics for your files.",
  "main": "src/index.js",
  "bin": {
    "csc": "bin/cli.js"
  },
  "scripts": {
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "test": "qunit tests/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajasegar/compression-stats-cli.git"
  },
  "keywords": [
    "compression",
    "gzip",
    "brotli",
    "cli"
  ],
  "author": "Rajasegar Chandran <rajasegar.c@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajasegar/compression-stats-cli/issues"
  },
  "homepage": "https://github.com/rajasegar/compression-stats-cli#readme",
  "dependencies": {
    "chalk": "^4.0.0",
    "filesize": "^6.1.0",
    "walk-sync": "^2.1.0",
    "workerpool": "^6.0.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "execa": "^4.0.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "qunit": "^2.10.0",
    "semantic-release": "^17.0.8"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "release(version): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">= 10.16.0"
  }
}
